<h1><%= @user.username %></h1>

    <dl class="dl-horizontal">
      <% @user.photos.each do |photo| %>
      <dt>Caption</dt>
      <dd><%= photo.caption %></dd>

      <dt>Image</dt>
      <dd>img[src*='#{<%= photo.image %>}']</dd>
      <!--<dd><image src="" class="img-responsive" ></image></dd>-->
      <% end %>

    </dl>